<html>
<head>
    <meta name="viewport" content="width=device-width" />
   
    <title>Home</title>
</head>
<body>
    <a href="~/Admin/AddProfile">Add</a>
    <a class="btn btn-info-grey" href="~/Admin/GetEpicoreOrder">Go For Order Assign</a>
    <a class="btn btn-info-grey" href="~/Admin/GetItemWithBinLocation">Show Binlocation </a>
    <table class="table">
        <thead>
            <tr>
                @*<th>
                        @Html.DisplayNameFor(model => model.Empid)
                    </th>*@
                <th>
                    Pickers
                </th>
                <th>
                    Email
                </th>
                <th>
                    Role
                </th>
                <th>
                    Company
                </th>

                <th>
                    Max lines
                </th>
                <th>
                    Max Weight
                </th>
                <th>
                    Line per hour
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @*@foreach (var item in ViewBag.Pickers as List<DotIt.AutoPicker.Models.PickerModel>)*@
            @foreach (var item in ViewBag.Pickers as List<DotIt.AutoPicker.Models.PickerModel>)
            {
                <tr>
                    <td>

                        @item.Name
                    </td>
                    <td>
                        @item.EMailAddress
                    </td>
                    <td>
                        <div style="width:200px;overflow-wrap:break-word">
                            @item.Grouplist
                        </div>

                    </td>
                    <td>
                        @item.Company
                    </td>

                    <td>
                        @item.MaxLines
                    </td>
                    <td>
                        @item.MaxWeight
                    </td>
                    <td>
                        @item.AdminlineperHour
                    </td>
                    <td>
                        <a href="~/Admin/EditProfile/@Constant.Encode(item.DcdUserID)">Edit</a>
                        @*<a href="~/Admin/DeleteProfile/@Constant.Encode(item.DcdUserID,false)">Delete</a>*@
                    </td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>

